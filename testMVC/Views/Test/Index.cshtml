@{
    ViewData["Title"] = "Test controller";
}
<h2>Đây là danh sách nhân vật</h2>
<a asp-action="Create">Create new person</a>
<table style="width:50%">
    <thead>
        <tr>
            <th>Workline</th>
            <th>TargetQty</th>
            <th>OutputQty</th>
            <th>QcOutputQty</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Item e in ViewBag.items)
        {
            <tr>
                <td>@e.Workline</td>
                <td>@e.TargetQty</td>
                <td>@e.OutputQty</td>
                <td>@e.QcOutputQty</td>
            </tr>
        }
    </tbody>
